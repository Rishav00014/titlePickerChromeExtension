{
  "manifest_version": 3,
  "name": "Title Picker",
  "description": "Auto reacter and commenter",
  "version": "3.0",
  "permissions":["tabs","activeTab","nativeMessaging"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/feed/"],
      "js": ["content-script.js"]
    }
  ],
  "action": {
    "default_popup": "pick.html",
    "default_icon": "logo.png"
  }
}