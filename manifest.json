{
  "manifest_version": 3,
  "name": "Title Picker",
  "description": "Linkedin bot",
  "version": "2.0",
  "permissions":["tabs","activeTab","nativeMessaging"],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/in/*"],
      "js": ["content-script.js"]
    }
  ],
  "action": {
    "default_popup": "pick.html",
    "default_icon": "logo.png"
  }
}